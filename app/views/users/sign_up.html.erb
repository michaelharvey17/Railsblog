<h1> Sign Up </h1>
<%= form_for User.new do |f| %>

  
	<div>
	  <%= f.label :username %>
	  <%= f.text_field :username %>
    </div>

    <div>
	  <%= f.label :fname, "First Name" %>
	  <%= f.text_field :fname %>
    </div>

    <div>
	  <%= f.label :lname, "Last Name" %>
	  <%= f.text_field :lname %>
    </div>

    <div>
	  <%= f.label :email %>
	  <%= f.email_field :email %>
    </div>

    <div>
	  <%= f.label :password %>
	  <%= f.password_field :password %>
    </div>

    <div>
	  <%= f.label :password_confirmation %>
	  <%= f.password_field :password_confirmation %>
    </div>

    <div>
	  <%= f.submit "Sign Up" %>
    </div>

<% end %>
